  display:block;
}
.barViewpro{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.barViewproBox{
  width: 23%;
  margin-left:12rpx;
  box-sizing: border-box;
}
.barViewproBox view{
  text-align: center;
