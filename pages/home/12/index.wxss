/* 推荐 */

.recommend {
  width: 100%;
}
/* 首页公共 */
page,view,text,image{
  box-sizing: border-box;
} 
.bar {
  width: 100%;
  height:100rpx;
  line-height: 100rpx;
}
.costumeImg{
  width: 20rpx;
  height: 60rpx;
  background: red;
  vertical-align:middle;
  margin:0 20rpx;
}

.classView{
  width:100%;
  display: flex;
  flex-wrap: wrap;
  background: #fff;
  padding-bottom:30rpx;
}

.classViewList{
  width:25%;
  /* height:141rpx; */
  text-align: center;
}
.classViewList image{
  width: 100rpx;
  height: 100rpx;
  display: block;
  margin:30rpx auto 0 auto;
  border-radius:50%;
  box-shadow: #ccc 2rpx  2rpx 10rpx;
}
.bar .barimg {
  width: 100%;
  height: 100%;
}
.biaoti{
  position:absolute;
  top:0;
  left:24rpx;
  line-height:88rpx;
  color:#fff;
}
.da{
  font-weight: bold;
}
/*产品  */
.barViewproImg{
  width:100%;
  border-radius:20rpx;
