/*精品*/
 @import "../../../template/goods-list/index.wxss";
 @import "../../../template/banner/index.wxss"; 
.search {
  width: 100%;
  height: 100rpx;
  background: #fff;
  position: relative;
  border-top:1rpx solid #f3f4f5;
}
.searchimg {
  width: 670rpx;
  height:60rpx;
  background: #e5e5e5;
  margin:20rpx auto;
  border-radius: 10rpx;
}

.searchText {
  width: 600rpx;
  height: 60rpx;
  position: absolute;
  top:22rpx;
  left: 38rpx;
}
.searchTextImg{
  width:24rpx;
  height:24rpx;
   margin:21rpx 20rpx 15rpx 24rpx;  
  float:left;
}
.searchTextwords{
   float:left;
   line-height:60rpx;
   color:#cdcdcd;
   font-size:26rpx;
}

/* 轮播图 */

.banner {
  width: 100%;
}


/* 预约 start */
.make {
  width: 100%;
  height: 88rpx;
  background: #fff; 
}

.makeTal {
  width: 100%;
  height: 88rpx;
  position: relative;
}

.makeimage {
  width: 28rpx;
  height: 26rpx;
  margin: 30rpx 16rpx 0 26rpx;
}

.maketext {
  line-height: 88rpx;
  color: #fe860f;
}

.makebut {
  position: absolute;
  width: 88rpx;
  height: 36rpx;
  top: 26rpx;
  right: 30rpx;
  color: #ff8804;
  border: 1rpx solid #ff8804;
  text-align: center;
  line-height: 32rpx;
  border-radius: 5rpx;
  font-size: 24rpx;
}
/* 预约 end */



/*推荐分类  */
.recommend{
  display: flex;
}

.make1 {
  width: 100%;
  background: #fff;
}

.makeTal1 {
  width: 100%;
  height: 100rpx;
  text-align: center;
}
.makeTals{
  width: 100%;
  text-align: center;
}
.makeimage1 {
  width: 58rpx;
  height: 4rpx;
  vertical-align: middle;
  padding-bottom: 10rpx;
}

.maketext1 {
  line-height: 88rpx;
  font-size: 32rpx;
  margin: 0 22rpx;
}

.customized{
  display: flex;
}

.makeTalTowBox{
  padding:0 20rpx;
  display: flex;
  flex-wrap: wrap;
  text-align: center;
}

.makeTalTowImg{
  width: 120rpx;
  height: 120rpx;
}

.makeTalTow {
  margin-bottom: 20rpx;
  width: 25%;
  padding: 0 20rpx;
}

.makeTalTow view{
  width: 150rpx;
  font-size: 30rpx;
  text-align: center; 
  overflow: hidden;
  white-space: nowrap; 
  text-overflow: ellipsis;   
}

/*zhuti  */
.caseMainBox {
  white-space: nowrap;
  display: flex;
  height: 203rpx;
  padding: 10rpx 0 30rpx 0;
}
.brandBox{
  white-space: nowrap;
  display: flex;
  height: 153rpx;
  padding: 0 0 35rpx 0;
}
.caseMain {
  display: inline-block;
}
.caseMainImg {
  width: 375rpx;
  height: 203rpx;
  background:rebeccapurple;
}
.caseMainImg1 {
  width: 375rpx;
  height: 203rpx;
  background:yellow;
 
}
.brandImg{
  width: 153rpx;
  height: 153rpx;
  background:rebeccapurple;
  border: 1rpx solid red;
}
.myOne{
  width: 100%;
  height: 30rpx;
  background: #e6e6e6;
}

/*热卖  */
.likeBox{
  display: flex;
  padding-left:40rpx; 
  flex-wrap: wrap;
}

.like{
  box-sizing: border-box;
  margin-right: 25rpx;
}
.likeImg{
  width:324rpx;
  height: 350rpx;
  border-radius: 10rpx;
}
.likeFont{
  width: 306rpx;
  font-size: 26rpx;
  margin:10rpx auto 0 auto;
  display:-webkit-box;
  overflow:hidden;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  line-height:1.5;
}
.likeOne{
  color: #fe0000;
  font-size: 24rpx;
  margin-right: 13rpx;
}
.likeTow{
  color: #8f8f8f;
  font-size: 24rpx;
}