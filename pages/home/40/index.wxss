/**index.wxss**/
@import "../../../template/banner/index.wxss";
.search {
  padding: 16rpx 30rpx;
  background: #37373b;
}
.searchimg {
  width: 600rpx;
  height:60rpx;
  background: #e5e5e5;
  margin:14rpx auto;
  border-radius: 30rpx;
}
.searchText {
  width: 100%;
  background: #fff;
  border-radius: 6rpx;
  height: 60rpx;
  line-height: 60rpx;
  display: flex;
  justify-content: center;
  color: #999;
}
.searchTextImg{
  width:30rpx;
  height:30rpx;
  margin:15rpx 20rpx 0;  
}
.container {
  background-color: #F2f2f2;
  min-height: 100%
}
.swiper-container{
  width: 750rpx;
  position: relative;  
  margin-bottom: 20rpx;
}
.swiper_box {
  width: 100%;
  height:410rpx;
}

swiper-item image {
  width: 100%;
  display: inline-block;
  overflow: hidden;
  height:410rpx;
}
.swiper-container .dots{  
  position: absolute;  
  left: 0;  
  right: 0;  
  bottom: 20rpx;  
  display: flex;  
  justify-content: center;  
}  
.swiper-container .dots .dot{  
  margin: 0 8rpx;  
  width: 14rpx;  
  height: 14rpx;  
  background: #fff;  
  border-radius: 50%;  
  transition: all .6s;  
  opacity: .5;
}  
.swiper-container .dots .dot.active{  
  width: 14rpx;  
  opacity: 1;
}

.index_top{
  /* height: 270rpx; */
  width: 100%;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.index_top image{
  vertical-align: top;
  width: 100%;
}
.index_one{
  height: 320rpx;
  overflow: hidden;
  display: flex;
  justify-content: space-between;
  flex-wrap: nowrap;
  align-items: center;
  margin-bottom: 20rpx;
}
.index_one_left{
   margin-right: 20rpx; 
   
}
.index_one_left,.index_one_right{
  flex: 1;
}
.index_one_right image{
  width: 100%;
}

.index_one_left image{
  width: 100%;
  height: 50rpx;
}
.index_one_right image{
  height: 160rpx;
  margin-bottom: 20rpx;
  float: left;
}
.index_two{
  width: 100%;
  /* height: 310rpx; */
  /* overflow: hidden; */
}
.index_two image{
  width: 100%;
}