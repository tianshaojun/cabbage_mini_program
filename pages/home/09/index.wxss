/* 头部 start */
.indexTop{
  width: 100%;
  position: relative;
}
.indexBg{
  width: 100%;
  float: left;
}
.indexHeader{
  position: absolute;
  text-align: center;
  width: 100%;
  top: 20rpx;
}
.indexIcon{
  position: absolute;
  left: 20rpx;
  top: 5rpx;
  font-size: 24rpx;
  color: #fff;
}
.indexName{
  color: #fff;
  font-size: 32rpx;
}
.indexImg{
  width: 160rpx;
  height: 160rpx;
  border:4rpx solid #fff;
  border-radius: 50%;
  margin-top: 20rpx;
}
.indexTitle{
  margin-top: 180rpx;
  font-size: 26rpx;
  color: #ff5e8e;
}
.indexTime{
  font-size: 24rpx;
  color: #666;
  margin: 34rpx 0;
}
.indexBtns navigator{
  height: 60rpx;
  line-height: 60rpx;
  color: #666;
  font-size: 28rpx;
  border: 1rpx solid #666;
  margin: 0 20rpx;
  padding: 0 20rpx;
}
/* 头部 end */

/* 预约 start */

.make {
    width: 100%;
    height: 88rpx;
    background: #fff;
    /* margin-top: 20rpx; */
}
.makeTal {
    width: 100%;
    height: 88rpx;
    position: relative;
}
.makeimage {
    width: 28rpx;
    height: 26rpx;
    margin: 30rpx 16rpx 0 26rpx;
}
.maketext {
    line-height: 88rpx;
    color: #fe860f;
}
.makebut {
    position: absolute;
    width: 88rpx;
    height: 36rpx;
    top: 26rpx;
    right: 30rpx;
    color: #ff8804;
    border: 1rpx solid #ff8804;
    text-align: center;
    line-height: 32rpx;
    border-radius: 5rpx;
    font-size: 24rpx;
}
/* 预约 end  */

/* 分类 start */
.indexCategory{
  position: relative;
  overflow:auto;
}
.indexCategory_bg{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.indexCategory_box{
  margin-top: 20rpx;
}
.indexCategory_item{
  position: relative;
  width: 33.33%;
  height: 270rpx;
  margin-bottom: 20rpx;
}
.item_img{
  width: 100%;
  height: 100%;
}
.item_title{
  position: absolute;
  bottom: 0;
  width: 100%;
  background: rgba(0,0,0,.5);
  font-size: 24rpx;
  line-height: 40rpx;
  text-align: center;
  color: #fff;
}

.footerImg{
  width: 100%;
  height: 100%;
  margin-bottom: 20rpx;
  vertical-align: top;
}
/* 分类 end */
 