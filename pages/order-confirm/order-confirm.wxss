page{
  background: #F5F5F5;
}
view {
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}
.box{padding-bottom:100rpx}
/*中间商品s*/
.goodLists {
  padding: 20rpx;
  background:#ffffff;
  overflow: hidden;
  border-bottom: 1px solid #c8c8c8; 
}
.goodDistribution {
  margin-top: 20rpx;
}
.goodDistribution .goodLists:last-child{
  border-bottom: 0;
}
/*中间商品e*/
/*文本输入s*/
.textArea{
  position: relative;
}
.textArea .textNum{
  position: absolute;
  color: #999999;
  right: 19rpx;
  bottom: 0;
}
.note{
  width: 749rpx;
  height: 160rpx;
  margin-top: 20rpx;
  background:#ffffff;
  padding: 20rpx 20rpx 40rpx 20rpx;
} /*文本输入e*/
.acceptInfo {
  width: 750rpx;
  min-height: 98rpx;
  background: #fff;
  font-size: 28rpx;
  margin-top: 20rpx;
  position: relative;
  line-height: 98rpx;
}
.getdefaultaddress{
  height:110rpx;
  width: 750rpx;
  background: #fff;
  font-size: 28rpx;
  margin-top: 20rpx;
  position: relative;
}
.noAddressView{
  overflow : hidden;
text-overflow: ellipsis;
display: -webkit-box;
-webkit-line-clamp: 1;
-webkit-box-orient: vertical;
}
 .acceptImg {
  position: absolute;
  top:35rpx;
  left:15rpx;
  width: 40rpx;
  height: 40rpx;
  /* margin-left: 15rpx;
  margin-top: 35rpx; 
  float: left;  */
}
.noAddress{
  padding:0 70rpx;
}
.noAddress .add{
  color:#e94746;
}
.acceptRight{
  position: absolute;
  top:35rpx;
  right:23rpx;
  width:14rpx;
  height:24rpx;
}
.bottomImg {
  display: block;  
  width: 750rpx;
  height: 6rpx; 
  position: absolute;
  bottom: 0;
}
.goodLeft {
  width: 140rpx;
  height: 140rpx;
  float: left;
}
.goodRight {
  width: 520rpx;
  height: 150rpx;
  float: left;
  margin-left: 15rpx;
}
.goodLeftImg {
  width: 140rpx;
  height: 140rpx;
}
.goodTxt {
  font-size: 28rpx;
  width: 538rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:1;
  overflow: hidden;
}
.price {
  height: 50rpx;
  text-align: left;
  line-height: 50rpx;
  font-size: 30rpx;
  color: #ff0077;
  float: left;
}
.number {
  margin-left: 10rpx;
  height: 50rpx;
  text-align: right;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #a6a6a6;
  float: left;
}
.AllMony{
  background:#ffffff;
  margin-top: 20rpx;
}
.descr{
  width: 750rpx;
  height: 88rpx;
  font-size: 28rpx;
  border-bottom: 1px solid #c8c8c8;
  line-height: 88rpx;
}
.descr:last-child{
  border-bottom: 0;
}
.goodAllDescr{
  display: block;
  float: left;
  margin-left: 22rpx;
  color: #666666;
}
.goodAllMony{
  display: block;
  float: right;
  margin-right: 21rpx;
  color: #999999;
}
.default{
  float: right;
  width:14rpx;
  height:24rpx;
  margin-top: 32rpx;
  margin-right: 30rpx;
}
/*底部s*/
.bottom {
  background: #ffffff ;
  width: 750rpx;
  height: 98rpx;
  line-height: 98rpx;
  position: fixed;
  bottom: 0;
  z-index: 999;
}
.allGoods{
  float:left;
  margin-left: 22rpx;
  color: #666666;
}
.allGoods .num{
  color:#ff0077;
}
.give{
  float: right;
  margin-right: 30rpx;
  color: #666666;
}
.give .mony{
  color:#ff0077;
}
.submitOrder{
  width: 210rpx;
  height: 98rpx;
  line-height: 98rpx;
  text-align: center;
  background: #fb5660;
  color: #ffffff;
  float: right;
  font-size: 30rpx;
}

/*底部e*/

